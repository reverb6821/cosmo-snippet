{
  "name": "starter-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup:project": "pnpm install && pnpm prepare",
    "precommit": "lint-staged",
    "prepare": "npx husky && npx husky install",
    "app:dev": "vite",
    "app:build": "tsc && vite build",
    "app:preview": "vite preview",
    "app:generate": "npx plop --plopfile ./generator/plopfile.js",
    "app:test": "jest",
    "app:test:coverage": "pnpm app:test --coverage --watchAll --collectCoverageFrom='src/components/**/*.{ts,tsx}'",
    "app:lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "app:lint:fix": "eslint . --ext ts,tsx --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.22.3",
    "storybook-addon-react-router-v6": "^2.0.15"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1.2.25",
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@storybook/addon-a11y": "^8.0.4",
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/addon-links": "^8.0.4",
    "@storybook/addon-onboarding": "^8.0.4",
    "@storybook/blocks": "^8.0.4",
    "@storybook/manager-api": "^8.0.4",
    "@storybook/react": "^8.0.4",
    "@storybook/react-vite": "^8.0.4",
    "@storybook/test": "^8.0.4",
    "@storybook/theming": "^8.0.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transformer-svg": "^2.0.2",
    "lint-staged": "^15.2.2",
    "plop": "^4.0.1",
    "storybook": "^8.0.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  }
}
